<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.Activity_BookCall">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="52dp"
        android:background="@color/TopBack"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="274dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="2dp"
            android:contentDescription="@string/becker_icon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/constraintLayout6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/icon"
            app:tint="@color/white"
            tools:ignore="ImageContrastCheck" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout6"
            android:layout_width="94dp"
            android:layout_height="26dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="22dp"
            android:background="@color/system"
            android:singleLine="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="5dp"
                android:text="@string/status"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="VisualLintBounds" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="76dp"
        android:background="@color/TopBack"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">


        <ImageButton
            android:id="@+id/callsBtn"
            android:layout_width="100dp"
            android:layout_height="52dp"
            android:layout_marginBottom="12dp"
            android:background="@color/TopBack"
            android:contentDescription="@string/call_icon"
            android:onClick="onClickGoCalls"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/emailBtn"
            app:layout_constraintLeft_toRightOf="@id/emailBtn"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@android:drawable/stat_sys_phone_call"
            app:tint="#159101"
            tools:ignore="ImageContrastCheck" />

        <ImageButton
            android:id="@+id/emailBtn"
            android:layout_width="100dp"
            android:layout_height="55dp"
            android:layout_marginBottom="8dp"
            android:background="@color/TopBack"
            android:contentDescription="@string/email_icon"
            android:onClick="onClickGoEmail"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/conviviisBtn"
            app:layout_constraintStart_toEndOf="@+id/callsBtn"
            app:srcCompat="@android:drawable/ic_dialog_email"
            app:tint="#000000"
            tools:ignore="ImageContrastCheck" />

        <ImageButton
            android:id="@+id/conviviisBtn"
            android:layout_width="100dp"
            android:layout_height="62dp"
            android:layout_marginBottom="4dp"
            android:background="@color/TopBack"
            android:contentDescription="@string/conviviis_icon"
            android:onClick="onClickGoConviviis"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/authBtn"
            app:layout_constraintStart_toEndOf="@+id/emailBtn"
            tools:ignore="ImageContrastCheck"
            app:srcCompat="@drawable/conviviis"
            app:tint="@color/black"/>

        <ImageButton
            android:id="@+id/authBtn"
            android:layout_width="100dp"
            android:layout_height="59dp"
            android:layout_marginBottom="8dp"
            android:background="@color/TopBack"
            android:contentDescription="@string/settings_icon"
            android:onClick="onClickGoAunth"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/conviviisBtn"
            app:srcCompat="@drawable/settings"
            app:tint="#000000"
            tools:ignore="ImageContrastCheck" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ListView
        android:id="@+id/listView_BookCall"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="622dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout11" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout10"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="60dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/ButtonLastCall"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rectangle"
            android:contentDescription="@string/ButtonLastCall"
            android:onClick="onClickGoLastCall"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ButtonNumberCall2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/phone_call"
            tools:ignore="DuplicateSpeakableTextCheck,ImageContrastCheck" />

        <ImageButton
            android:id="@+id/ButtonNumberCall2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="@drawable/shape_circle"
            android:contentDescription="@string/ButtonNumberCall"
            android:onClick="onClickGoNumber"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/number_dialing" />

        <ImageButton
            android:id="@+id/ButtonBookCall"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rectangle"
            android:contentDescription="@string/ButtonBookCall"
            android:onClick="onClickGoBookCall"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.56"
            app:layout_constraintStart_toEndOf="@+id/ButtonNumberCall2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.526"
            app:srcCompat="@drawable/phonebook"
            app:tint="@color/Green"
            tools:ignore="ImageContrastCheck" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout11"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="40dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="29dp"
            android:layout_height="31dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.555"
            app:srcCompat="@drawable/magnifier_icon"
            android:contentDescription="@string/SerchIcon" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="313dp"
            android:layout_height="match_parent"
            android:autofillHints=""
            android:ems="10"
            android:inputType="text"
            android:hint="@string/SerchClient"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView2"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="LabelFor,TouchTargetSizeCheck" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="46dp"
            android:layout_height="match_parent"
            android:contentDescription="@string/BookCall"
            android:onClick="onClickGoAddUser"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/add_user"
            tools:ignore="TouchTargetSizeCheck" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>